<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:sofa="http://sofastack.io/schema/sofaboot"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
            http://sofastack.io/schema/sofaboot http://sofastack.io/schema/sofaboot.xsd"
       default-autowire="byName">
    <sofa:reference jvm-first="false" id="dubboServiceReference" interface="com.alipay.sofa.rpc.samples.dubbo.DubboService">
        <sofa:binding.dubbo/>
    </sofa:reference>


    <sofa:reference jvm-first="false" id="dubboServiceReference1" interface="com.alibaba.dubbo.rpc.service.GenericService">
        <sofa:binding.dubbo>
            <sofa:global-attrs generic-interface="com.alipay.sofa.rpc.samples.dubbo.DubboService" />
        </sofa:binding.dubbo>
    </sofa:reference>
</beans>