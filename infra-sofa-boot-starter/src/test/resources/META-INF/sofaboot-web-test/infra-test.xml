<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:sofa="http://schema.alipay.com/sofa/schema/slite"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
            http://schema.alipay.com/sofa/schema/slite   http://schema.alipay.com/sofa/slite.xsd"
       default-autowire="byName">

    <bean id="sofaBootVersionImpl" class="com.alipay.sofa.infra.bean.SofaBootVersionImpl"/>

    <bean id="sofaRestVersionImpl" class="com.alipay.sofa.infra.bean.SofaRestVersionImpl"/>

    <bean id="sofaRpcVersionImpl" class="com.alipay.sofa.infra.bean.SofaRpcVersionImpl"/>

</beans>