<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="proxyTestBean" class="com.alipay.sofa.smoke.tests.runtime.proxy.ProxyTestBeanImpl" />

    <bean id="proxyFactoryBean1" class="org.springframework.aop.framework.ProxyFactoryBean" >
        <property name="proxyInterfaces" value="com.alipay.sofa.smoke.tests.runtime.proxy.ProxyTestBeanFacade" />
        <property name="target" ref="proxyTestBean" />
    </bean>

    <bean id="proxyFactoryBean2" class="org.springframework.aop.framework.ProxyFactoryBean" >
        <property name="interfaces" value="com.alipay.sofa.smoke.tests.runtime.proxy.ProxyTestBeanFacade" />
        <property name="target" ref="proxyTestBean" />
    </bean>

    <bean id="proxyFactoryBean3" class="org.springframework.aop.framework.ProxyFactoryBean" >
        <property name="target" ref="proxyTestBean" />
        <property name="targetName" value="proxyTestBean" />
        <property name="targetClass" value="com.alipay.sofa.smoke.tests.runtime.proxy.ProxyTestBeanFacade"/>
    </bean>

    <bean id="proxyFactoryBean4" class="org.springframework.aop.framework.ProxyFactoryBean" >
        <property name="target" ref="proxyTestBean" />
        <property name="targetClass" value="com.alipay.sofa.smoke.tests.runtime.proxy.ProxyTestBeanFacade"/>
    </bean>

    <bean class="com.alipay.sofa.smoke.tests.runtime.proxy.ProxyTestPostProcessor" />

</beans>