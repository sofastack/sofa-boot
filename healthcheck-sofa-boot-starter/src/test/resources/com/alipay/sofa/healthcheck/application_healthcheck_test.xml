<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


    <bean id="referenceA" class="com.alipay.sofa.healthcheck.bean.ReferenceA"/>

    <bean id="referenceB" class="com.alipay.sofa.healthcheck.bean.ReferenceB"/>

    <bean id="healthIndicatorA" class="com.alipay.sofa.healthcheck.bean.HealthIndicatorA"/>

    <bean id="healthIndicatorB" class="com.alipay.sofa.healthcheck.bean.HealthIndicatorB"/>

    <bean id="afterHealthCheckCallbackA" class="com.alipay.sofa.healthcheck.bean.AfterReadinessCheckCallbackA"/>

    <bean id="afterHealthCheckCallbackB" class="com.alipay.sofa.healthcheck.bean.AfterReadinessCheckCallbackB"/>
</beans>